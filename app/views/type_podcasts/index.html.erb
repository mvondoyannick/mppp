<p id="notice"><%= notice %></p>

<h1>Type Podcasts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_podcasts.each do |type_podcast| %>
      <tr>
        <td><%= type_podcast.name %></td>
        <td><%= link_to 'Show', type_podcast %></td>
        <td><%= link_to 'Edit', edit_type_podcast_path(type_podcast) %></td>
        <td><%= link_to 'Destroy', type_podcast, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Podcast', new_type_podcast_path %>
